import java.io.*;
public class Variables2 {

	// Inicializar Array de 3 dimensiones y medir longitud
	public void array (){
	
		int arreglo [][][] = {{{8, 20, 4, 4},{4, 13, 34, 8}},{{4, 30, 4, 2},{7, 12, 54, 8}}};
		
		
		System.out.println("La persona escogida tiene: "+arreglo[0][0][4]+" de edad");
		
		
		
	}
				
		
		
		
		
	
	
	public static void main(String[] args) {
		
Variables2 re = new Variables2();

re.array();
	}

}
